    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache .turbo .swc",
    "clean:all": "pkill -f 'next dev' 2>/dev/null; pkill -f 'next-server' 2>/dev/null; lsof -ti:3000 | xargs kill -9 2>/dev/null; rm -rf .next node_modules/.cache .turbo .swc",
    "sync": "bash scripts/sync-localhost-to-production.sh",
    "sync-and-deploy": "bash scripts/sync-localhost-to-production.sh && firebase deploy --only hosting:app-micerebro",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel"
