    "dev": "next dev -p 3001",
    "dev:clean": "rm -rf .next node_modules/.cache .turbo && next dev -p 3001",
    "dev:radical": "./clean-dev.sh",
    "build": "next build",
    "build:hosting": "next build && node scripts/post-build.js",
